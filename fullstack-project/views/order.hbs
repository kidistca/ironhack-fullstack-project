<h1>Ola order place</h1>

<div class="seller-products">
{{#each products}}
<div class="card col-4" style="width: 18rem;">
  <img src="{{this.resizedUrl}} " class="card-img-top" alt="{{this.description}}">
  <div class="card-body">
      <h5 class="card-title">Card title</h5> 
      <p class="card-text">{{this.description}}</p> 
      <p>{{this._id}}</p>
      <p>$ {{this.price}}</p>
      <form action="/order">
         <button class="btn btn-dark">Order</button>
      </form>
    </ul>
  </div>
</div>
{{/each}}
