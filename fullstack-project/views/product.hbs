{{!-- <div class="seller-products"> --}}
  <div class="container pt-5">
  <div class="row">
{{#each products}}

    <div class="col-lg">
    <div class="card" style="width: 21rem;">
      <img src="{{this.resizedUrl}} " class="card-img-top" alt="{{this.description}}">
    <div class="card-body">
      <h5 class="card-title">Card title</h5> 
      <p class="card-text">{{this.description}}</p> 
      <p>{{this._id}}</p>
      <p>$ {{this.price}}</p>
      <form action="/order">
         <button class="btn btn-dark btn-lg" value=this._id>Order</button>
      </form>
    </ul>
  </div>
</div>
</div>
{{/each}}
